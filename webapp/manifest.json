{
	"_version": "1.32.0",
	"sap.app": {
		"id": "ui5.echarts.app",
		"type": "application",
		"i18n": "",
		"title": "UI5 ECharts Application",
		"description": "SAP UI5 application with ECharts integration",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.143.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"models": {},
		"dataSources": {
			"northwind": {
				"uri": "https://services.odata.org/V4/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"rootView": {
			"viewName": "ui5.echarts.app.view.Main",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentName": "ui5.echarts.app",
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ui5.echarts.app.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true,
				"bypassed": {
					"target": "chartList"
				}
			},
			"routes": [
				{
					"name": "chartList",
					"pattern": "",
					"target": "chartList"
				},
				{
					"name": "chartDetail",
					"pattern": "chart/{chartType}",
					"target": "chartDetail"
				},
				{
					"name": "optimization",
					"pattern": "optimization/{scenario}",
					"target": "optimization"
				}
			],
			"targets": {
				"chartList": {
					"viewName": "ChartList",
					"viewId": "chartList",
					"viewLevel": 1
				},
				"chartDetail": {
					"viewName": "ChartDetail",
					"viewId": "chartDetail",
					"viewLevel": 2
				},
				"optimization": {
					"viewName": "Optimization",
					"viewId": "optimization",
					"viewLevel": 2
				}
			}
		}
	}
}