{
  "name": "ui5-echarts-app",
  "version": "1.0.0",
  "main": "index.js",
  "description": "OpenUI5 project integrated with Apache ECharts",
  "scripts": {
    "start": "ui5 serve --config=ui5.yaml --port 8080 -o index.html",
