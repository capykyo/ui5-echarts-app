  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@ui5/cli": "^4.0.39",
    "ui5-middleware-livereload": "^3.2.1",
    "ui5-tooling-modules": "^3.34.2"
  },
  "dependencies": {
    "echarts": "^6.0.0",
    "json-server": "^1.0.0-beta.3"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-tooling-modules"
    ]
  }
